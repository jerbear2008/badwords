{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node"
    },
    {
        "src": "web/**",
        "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    },
    {
        "src": "/web/home",
        "dest": "web/home/index.html"
    },
    {
        "src": "/web/(.+)",
        "dest": "web/$1"
    }
  ]
}
