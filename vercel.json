{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node"
    },
    {
        "src": "web/**",
        "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api",
      "dest": "index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    },
    {
        "src": "/",
        "dest": "web/home"
    },
    {
        "src": "/(.*(css|js))",
        "dest": "web/home/$1"
    },
    {
        "src": "/assets",
        "dest": "web/home/assets/$1"
    },
    {
        "src": "/(.+)",
        "dest": "web/$1"
    },
    {
        "src": "/(.+)/(.+)",
        "dest": "web/$1/$2"
    }
  ]
}
