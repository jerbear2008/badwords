{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
        "src": "web/**/*",
        "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/home/"
    },
    {
      "source": "/docs",
      "destination": "/docs"
    }
  ],
  "routes": [
    {
      "src": "/api",
      "dest": "index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/(.+)",
      "dest": "web/$1"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
